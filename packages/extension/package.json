{
  "name": "busy-bee-vs",
  "displayName": "Busy-Bee-VS",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.107.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "busy-bee-vs.helloWorld",
        "title": "Hello World"
      },
      {
        "command": "busy-bee-vs.showFileSwitchStats",
        "title": "Show File Switch Stats"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "busy-bee-container",
          "title": "Busy Bee",
          "icon": "./resources/bee-icon.svg"
        }
      ]
    },
    "views": {
      "busy-bee-container": [
        {
          "icon": "resources/bee-icon.png",
          "type": "webview",
          "id": "busyBee.productDashboard",
          "name": "Product Dashboard"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "node esbuild.js",
    "watch": "npm-run-all -p watch:*",
    "watch:esbuild": "node esbuild.js --watch",
    "watch:tsc": "tsc --noEmit --watch --project tsconfig.json",
    "package": "npm run check-types && npm run lint && node esbuild.js --production",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "check-types": "tsc --noEmit",
    "lint": "eslint src",
    "test": "vscode-test",
    "dev": "node esbuild.js --watch",
    "build:dashboard": "node ../../scripts/build-dashboard.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.107.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "22.x",
    "typescript-eslint": "^8.48.1",
    "eslint": "^9.39.1",
    "esbuild": "^0.27.1",
    "npm-run-all": "^4.1.5",
    "typescript": "^5.9.3",
    "@vscode/test-cli": "^0.0.12",
    "@vscode/test-electron": "^2.5.2"
  },
  "dependencies": {
    "axios": "^1.7.9"
  }
}
